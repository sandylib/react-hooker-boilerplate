{
  "name": "react-hooker-boilerplate",
  "version": "0.1.2",
  "main": "dist/index.js",
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sandylib/react-hooker-boilerplate.git"
  },
  "keywords": [
    "react",
    "react-hook",
    "react-hooks",
    "context-api",
    "useContext",
    "useReducer",
    "context api"
  ],
  "author": "sandylib",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sandylib/react-hooker-boilerplate/issues"
  },
  "homepage": "https://github.com/sandylib/react-hooker-boilerplate#readme",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/components/index.js",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^9.1.4",
    "@testing-library/react-hooks": "^2.0.1",
    "enzyme": "^3.10.0",
    "generact": "^0.4.0",
    "generate-schema": "^2.6.0",
    "mock-json-schema": "^1.0.5",
    "sinon": "^7.4.2",
    "enzyme-adapter-react-16": "^1.14.0"
  }
